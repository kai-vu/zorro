@prefix : <http://Ameneh.org/aircraft.owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:ExcessiveOilConsumption rdfs:label "Excessive Oil Consumption" ;
    :hasCause :FailingOrFailedBearings,
        :FailureOfRingsToSeat,
        :IncorrectInstallationOfPistonRings,
        :LowGradeOfOil,
        :WornPistonRings ;
    rdfs:subClassOf :Problem .

:FailureOfEngineToDevelopFullPower rdfs:label "Failure of Engine to Develop Full Power" ;
    :hasCause :FaultyIgnition,
        :ImproperFuel,
        :ImproperFuelFlow,
        :LeakInInductionSystem,
        :RestrictionInAirScoop,
        :ThrottleLeverOutOfAdjustment ;
    rdfs:subClassOf :Problem .

:FailureOfEngineToIdleProperly rdfs:label "Failure of Engine to Idle Properly" ;
    :hasCause :FaultyIgnitionSystem,
        :IncorrectIdleAdjustment,
        :IncorrectIdleMixture,
        :InsufficientFuelPressure,
        :LeakInInductionSystem,
        :UnevenCylinderCompression ;
    rdfs:subClassOf :Problem .

:FailureOfEngineToStart rdfs:label "Failure of Engine to Start" ;
    :hasCause :DefectiveBattery,
        :DefectiveIgnitionWire,
        :DefectiveSparkPlugs,
        :ImproperOperationOfMagnetoBreaker,
        :InternalFailure,
        :LackOfFuel,
        :LackOfSufficientFuelFlow,
        :Overpriming,
        :WaterInCarburetor ;
    rdfs:subClassOf :Problem .

:LowOilPressure rdfs:label "Low Oil Pressure" ;
    :hasCause :AirLockOrDirtInReliefValve,
        :DefectivePressureGage,
        :HighOilTemperature,
        :InsufficientOil,
        :LeakInSuctionLineOrPressureLine,
        :StoppageInOilPumpIntakePassage ;
    rdfs:subClassOf :Problem .

:LowPowerAndUnevenRunning rdfs:label "Low Power and Uneven Running" ;
    :hasCause :DefectiveIgnitionWire,
        :DefectiveSparkPlugTerminalConnectors,
        :DefectiveSparkPlugs,
        :ImproperFuel,
        :LeaksInInductionSystem,
        :MagnetoBreakerPointsNotWorkingProperly,
        :MixtureTooLean,
        :MixtureTooRich ;
    rdfs:subClassOf :Problem .

:RoughEngine rdfs:label "Rough Engine" ;
    :hasCause :CrackedEngineMount,
        :DefectiveMountingBushings,
        :UnevenCompression ;
    rdfs:subClassOf :Problem .

