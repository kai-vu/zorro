@prefix : <http://Ameneh.org/aircraft.owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:ExcessiveOilConsumption a :Problem ;
    rdfs:label "Excessive Oil Consumption" ;
    :hasCause :FailingOrFailedBearings,
        :FailureOfRingsToSeat,
        :IncorrectInstallationOfPistonRings,
        :LowGradeOfOil,
        :WornPistonRings .

:FailureOfEngineToDevelopFullPower a :Problem ;
    rdfs:label "Failure of Engine to Develop Full Power" ;
    :hasCause :FaultyIgnition,
        :ImproperFuel,
        :ImproperFuelFlow,
        :LeakInInductionSystem,
        :RestrictionInAirScoop,
        :ThrottleLeverOutOfAdjustment .

:FailureOfEngineToIdleProperly a :Problem ;
    rdfs:label "Failure of Engine to Idle Properly" ;
    :hasCause :FaultyIgnitionSystem,
        :IncorrectIdleAdjustment,
        :IncorrectIdleMixture,
        :InsufficientFuelPressure,
        :LeakInInductionSystem,
        :UnevenCylinderCompression .

:FailureOfEngineToStart a :Problem ;
    rdfs:label "Failure of Engine to Start" ;
    :hasCause :DefectiveBattery,
        :DefectiveIgnitionWire,
        :DefectiveSparkPlugs,
        :ImproperOperationOfMagnetoBreaker,
        :InternalFailure,
        :LackOfFuel,
        :LackOfSufficientFuelFlow,
        :Overpriming,
        :WaterInCarburetor .

:LowOilPressure a :Problem ;
    rdfs:label "Low Oil Pressure" ;
    :hasCause :AirLockOrDirtInReliefValve,
        :DefectivePressureGage,
        :HighOilTemperature,
        :InsufficientOil,
        :LeakInSuctionLineOrPressureLine,
        :StoppageInOilPumpIntakePassage .

:LowPowerAndUnevenRunning a :Problem ;
    rdfs:label "Low Power and Uneven Running" ;
    :hasCause :DefectiveIgnitionWire,
        :DefectiveSparkPlugTerminalConnectors,
        :DefectiveSparkPlugs,
        :ImproperFuel,
        :LeaksInInductionSystem,
        :MagnetoBreakerPointsNotWorkingProperly,
        :MixtureTooLean,
        :MixtureTooRich .

:RoughEngine a :Problem ;
    rdfs:label "Rough Engine" ;
    :hasCause :CrackedEngineMount,
        :DefectiveMountingBushings,
        :UnevenCompression .

