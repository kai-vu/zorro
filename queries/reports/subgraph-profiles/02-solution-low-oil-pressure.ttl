        :Problem ;
    rdfs:label "Stoppage in oil pump intake passage" ;
    rdfs:subClassOf :Problem .

:LowOilPressure a :Problem ;
    rdfs:label "Low Oil Pressure" ;
    rdfs:subClassOf :Problem ;
